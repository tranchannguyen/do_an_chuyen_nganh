  // Deals of the week
  .deals_featured
    .container
      .row
        .col.d-flex.flex-lg-row.flex-column.align-items-center.justify-content-start
          // Deals
          .deals
            .deals_title Deals of the Week
            .deals_slider_container
              // Deals Slider
              .owl-carousel.owl-theme.deals_slider
                // Deals Item
                .owl-item.deals_item
                  .deals_image
                    img(src='/images/deals.png', alt='')
                  .deals_content
                    .deals_info_line.d-flex.flex-row.justify-content-start
                      .deals_item_category
                        a(href='#') Headphones
                      .deals_item_price_a.ml-auto $300
                    .deals_info_line.d-flex.flex-row.justify-content-start
                      .deals_item_name ZQ19
                      .deals_item_price.ml-auto $225
                    .available
                      .available_line.d-flex.flex-row.justify-content-start
                        .available_title
                          | Available: 
                          span 6
                        .sold_title.ml-auto
                          | Already sold: 
                          span 28
                      .available_bar
                        span(style='width:17%')
                    .deals_timer.d-flex.flex-row.align-items-center.justify-content-start
                      .deals_timer_title_container
                        .deals_timer_title Hurry Up
                        .deals_timer_subtitle Offer ends in:
                      .deals_timer_content.ml-auto
                        .deals_timer_box.clearfix(data-target-time='')
                          .deals_timer_unit
                            #deals_timer1_hr.deals_timer_hr
                            span hours
                          .deals_timer_unit
                            #deals_timer1_min.deals_timer_min
                            span mins
                          .deals_timer_unit
                            #deals_timer1_sec.deals_timer_sec
                            span secs
            .deals_slider_nav_container
              .deals_slider_prev.deals_slider_nav
                i.fas.fa-chevron-left.ml-auto
              .deals_slider_next.deals_slider_nav
                i.fas.fa-chevron-right.ml-auto
          // Featured
          .featured
            .tabbed_container
              .tabs
                ul.clearfix
                  li.active Featured
                  li On Sale
                  li Best Rated
                .tabs_line
                  span
              // Product Panel
              .product_panel.panel.active
                .featured_slider.slider
                  // Slider Item
                  each feature in featured
                    .featured_slider_item
                      .border_active
                      .product_item.is_new.d-flex.flex-column.align-items-center.justify-content-center.text-center
                        .product_image.d-flex.flex-column.align-items-center.justify-content-center
                          img(src= feature.pro_images[0],width = '160px' ,height= '150px')
                          br
                        .product_content
                          .product_price
                            span= feature.price + " $"
                          .product_name
                            div
                              a(href='/viewDetailProduct/'+feature._id)= feature.name
                          .product_extras
                            button.product_cart_button Add to Cart
                        .product_fav
                          i.fas.fa-heart
                        ul.product_marks
                          li.product_mark.product_new new
                          if feature.isSale.status == true
                            li.product_mark.product_discount= feature.isSale.percent + '%'
                  
              // Product Panel
              .product_panel.panel
                .featured_slider.slider
                  // Slider Item
                  each laptop in productSale
                    .featured_slider_item
                      .border_active
                      .product_item.discount.d-flex.flex-column.align-items-center.justify-content-center.text-center
