                        .product_content
                          .product_price
                            br
                            span= laptop.price + '$'
                          .product_name
                            div
                              a(href='/viewDetailProduct/'+laptop._id)= laptop.name
                          .product_extras
                            button.product_cart_button Add to Cart
                        .product_fav
                          i.fas.fa-heart
                        ul.product_marks
                          li.product_mark.product_new new
                          li.product_mark.product_discount= laptop.isSale.percent + '%'
              
